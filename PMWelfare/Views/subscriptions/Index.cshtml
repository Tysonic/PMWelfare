@model IEnumerable<PMWelfare.Models.Subscription>

@{
    ViewBag.Title = "Index";
}

<h2 style="text-align:center    ">Record Of Subscriptions</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Amount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubMonth.ToString("MMM"))
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubYear)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubMonth)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubYear)
            </td>

        </tr>
    }

</table>

